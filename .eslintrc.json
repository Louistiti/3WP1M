{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "no-trailing-spaces": "off",
        "linebreak-style": "off",
        "indent": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "no-param-reassign": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    }
}